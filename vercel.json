{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "AuthControllers/AuthController.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/signup",
      "dest": "AuthControllers/AuthController.js"
    },
    {
      "src": "/login",
      "dest": "AuthControllers/AuthController.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
